{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://gut-to-know-game-backend.onrender.com"
    },
      { "source": "/(.*)", "destination": "/" }],
      "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://gut-to-know.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ]
  }